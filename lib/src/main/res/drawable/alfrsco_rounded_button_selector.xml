<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:bottom="2dp" android:left="2dp" android:state_pressed="false">
        <shape>
            <gradient android:angle="0" android:endColor="?smColorButtonIcons" android:startColor="?smColorButtonIcons" />

            <stroke android:width="1dp" android:color="?smColorButtonIcons" />

            <corners android:radius="3dp" />

            <padding android:bottom="10dp" android:left="10dp" android:right="10dp" android:top="10dp" />
        </shape>
    </item>
    <item android:bottom="2dp" android:left="2dp" android:state_pressed="true">
        <shape>

            <gradient android:angle="0" android:endColor="?smColorButton" android:startColor="?smColorButton" />

            <stroke android:width="1dp" android:color="?smColorButton" />

            <corners android:radius="3dp" />

            <padding android:bottom="10dp" android:left="10dp" android:right="10dp" android:top="10dp" />
        </shape>
    </item>

</selector>